    "iacg-block": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "lego-components": "^0.1.7",
    "lodash": "^4.17.21",
    "mongoose": "^5.13.14",
    "mongoose-sequence": "^5.3.1",
    "nanoid": "^3.1.30",
    "release-it": "^15.1.3",
    "stream-wormhole": "^1.1.0",
    "vue": "^3.3.3"
  },
