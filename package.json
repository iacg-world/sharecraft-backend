    "@vue/server-renderer": "^3.5.26",
    "axios": "^0.27.2",
    "busboy": "^0.3.1",
    "dotenv": "^16.0.1",
    "egg": "^3.32.0",
    "egg-bcrypt": "^1.1.0",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.1",
    "egg-oss": "^2.0.0",
    "egg-redis": "^2.6.1",
    "egg-scripts": "^2.6.0",
    "egg-validate": "^2.0.2",
